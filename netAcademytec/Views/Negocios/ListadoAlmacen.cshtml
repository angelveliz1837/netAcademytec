@model IEnumerable<Domain.Entity.Almacen>

@{
    ViewBag.Title = "ListadoAlmacen";
    Layout = "~/Views/Shared/_Layout.cshtml";

}
<div class="row">
    <h2>ListadoAlmacen</h2>

    <p>
        @Html.ActionLink("Create New", "CreateAlmacen")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.idalmacen)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.idlibro)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.cantidad)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.idempleado)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.idalmacen)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.idlibro)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.cantidad)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.idempleado)
                </td>
                <td>
                    @Html.ActionLink("Edit", "EditAlmacen", new { id = item.idalmacen }) |
                    @Html.ActionLink("Details", "DetailsAlmacen", new { id = item.idalmacen }) |
                    @Html.ActionLink("Delete", "DeleteAlmacen", new { id = item.idalmacen })
                </td>
            </tr>
        }

    </table>
</div>
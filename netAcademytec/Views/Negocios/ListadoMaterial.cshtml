@model IEnumerable<Domain.Entity.Material>

@{
    ViewBag.Title = "ListadoMaterial";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row">
    <h2>ListadoMaterial</h2>

    <p>
        @Html.ActionLink("Create New", "CreateMaterial")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.idmaterial)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.nombrematerial)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.preciomaterial)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.idrecibo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.numerorecibo)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.idmaterial)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.nombrematerial)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.preciomaterial)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.idrecibo)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.numerorecibo)
                </td>
                <td>
                    @Html.ActionLink("Edit", "EditMaterial", new { id = item.idmaterial }) |
                    @Html.ActionLink("Details", "DetailsMaterial", new { id = item.idmaterial }) |
                    @Html.ActionLink("Delete", "DeleteMaterial", new { id = item.idmaterial })
                </td>
            </tr>
        }

    </table>
</div>